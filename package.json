{
  "homepage": "https://katesunday.github.io/todoList-ts/",
  "name": "todolist-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^7.0.3",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.1.2",
    "uuid": "^8.0.0",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "build-storybook": "build-storybook -s public",
    "jest:integration": "jest -c integration/jest.config.js",
    "test:integration": "start-server-and-test storybook http-get://localhost:9009 jest:integration"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/builder-webpack4": "^6.5.0",
    "@storybook/manager-webpack4": "^6.5.0",
    "@storybook/node-logger": "^6.5.0",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.5.0",
    "@storybook/testing-library": "^0.0.11",
    "gh-pages": "^4.0.0",
    "jest-image-snapshot": "^4.5.1",
    "jest-puppeteer": "^6.1.0",
    "puppeteer": "^14.1.0",
    "start-server-and-test": "^1.14.0"
  },
  "optionalDependencies": {
    "babel-loader": "8.1.0"
  }
}
